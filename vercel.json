{
  "version": 2,
  "functions": {
    "api/index.ts": {
      "runtime": "nodejs20.x",
      "includeFiles": "express-api/src/**"
    }
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist/apparel-store/browser",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
