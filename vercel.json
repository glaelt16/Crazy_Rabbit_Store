{
  "version": 2,
  "builds": [
    {
      "src": "express-api/src/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "angular.json",
      "use": "@vercel/angular",
      "options": {
        "outputPath": "dist/apparel-store/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/express-api/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
